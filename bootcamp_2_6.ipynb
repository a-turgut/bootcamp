    "def kenar(x,y):\n",
    "    dizi=np.ones((x,y))\n",
    "    for c in range(x-2):\n",
    "        dizi[c+1,1:y-1]=[0]\n",
    "    return dizi\n",
    "print(kenar(10,6))"
